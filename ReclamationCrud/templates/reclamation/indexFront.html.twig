                    <div class="collapse navbar-collapse" id="navbarNavDropdown">
                        <ul class="navbar-nav ml-auto">
                            <li><a href="JavaScript:void(0)">Pages</a>
                                <ul class="dropdown">
                                    <li><a href="JavaScript:void(0)">Reclamation</a>
                                        <ul class="dropdown dropdown-push-left">
                                            <li><a href="{{ path('app_reclamation_new') }}">Ajouter Reclamation</a></li>
                                            <li><a href="{{ path('app_reclamation_index') }}">Consulter Reclamation</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div><!-- /.container-->
        </div><!-- /.znav-container-->
        <section class="color-1 background-10 font-1 py-6">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8">
                        <h1 class="fs-3 fs-sm-4 fw-400">Reclamations</h1>
                    </div>
                </div><!--/.row-->
            </div><!--/.container-->
        </section>
        <section class="font-1 pt-5">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-8">
                        <div class="my-6"></div>
                        <h4>Table Reclamation</h4>
                        <div class="my-4"></div>
                        <div class="table-responsive">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Description</th>
                                        <th>Date effectue</th>
                                        <th>effectue sur</th>
                                        <th>effectue sur le nom</th>
                                        <th>effectue sur l'email</th>
                                        <th>option</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {% for reclamation in reclamations %}
            <tr>
                <td>{{ reclamation.id }}</td>
                <td>{{ reclamation.description }}</td>
                <td>{{ reclamation.dateCreation ? reclamation.dateCreation|date('Y-m-d') : '' }}</td>
                <td>{{ reclamation.type ? reclamation.type|json_encode : '' }}</td>
                <td>{{ reclamation.nom }}</td>
                <td>{{ reclamation.email }}</td>
                <td>
                    <a href="{{ path('app_reclamation_show_user', {'id': reclamation.id}) }}"class="btn btn-info btn-sm">show</a>
                    <a href="{{ path('app_reclamation_edit', {'id': reclamation.id}) }}"class="btn btn-info btn-sm">EDIT</a>
                    <a href="{{ path('app_reclamation_delete_front', {'id': reclamation.id}) }}"class="btn btn-danger btn-sm">Delete</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div><!--/.row-->
            </div><!--/.container-->
        </section>
        <footer class="background-11 color-1 py-3 fs--1 fw-600">
            <div class="container">
                <div class="row">
                    <div class="col-6 font-1">&copy; 2017 POSH &trade; Inc. </div>
                </div>
            </div>
        </footer>
    </main>
