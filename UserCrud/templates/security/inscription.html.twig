{% extends 'base.html.twig' %}

{% block title %}Sign UP!{% endblock %}

{% block body %}
<!DOCTYPE html>

<html lang="en" class="default-style layout-fixed layout-navbar-fixed">


<!-- Mirrored from html.phoenixcoded.net/empire/bootstrap/default/pages_authentication_register-v2.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 24 Mar 2023 20:57:06 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="description" content="Empire is one of the unique admin template built on top of Bootstrap 4 framework. It is easy to customize, flexible code styles, well tested, modern & responsive are the topmost key factors of Empire Dashboard Template" />
    <meta name="keywords" content="bootstrap admin template, dashboard template, backend panel, bootstrap 4, backend template, dashboard template, saas admin, CRM dashboard, eCommerce dashboard">
    <meta name="author" content="Codedthemes" />
    <link rel="icon" type="image/x-icon" href="assets/img/favicon.ico">
{% block css%}
    <!-- Google fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">

    <!-- Icon fonts -->
    <link rel="stylesheet" href="{{asset('back-office/fonts/fontawesome.css')}}">
    <link rel="stylesheet" href="{{asset('back-office/fonts/ionicons.css')}}">
    <link rel="stylesheet" href="{{asset('back-office/fonts/linearicons.css')}}">
    <link rel="stylesheet" href="{{asset('back-office/fonts/open-iconic.css')}}">
    <link rel="stylesheet" href="{{asset('back-office/fonts/pe-icon-7-stroke.css')}}">
    <link rel="stylesheet" href="{{asset('back-office/fonts/feather.css')}}">

    <!-- Core stylesheets -->
   <link rel="stylesheet" href="{{asset('back-office/css/bootstrap-material.css')}}">
    <link rel="stylesheet" href="{{asset('back-office/css/shreerang-material.css')}}">
    <link rel="stylesheet" href="{{asset('back-office/css/uikit.css')}}">

     <!-- Libs -->
        <link rel="stylesheet" href="{{asset('back-office/libs/perfect-scrollbar/perfect-scrollbar.css')}}">
    <!-- Page -->
    <link rel="stylesheet" href="{{asset('back-office/css/pages/authentication.css')}}">
    {% endblock %}
</head>

    <!-- [ Preloader ] Start -->
    <div class="page-loader">
        <div class="bg-primary"></div>
    </div>
    <!-- [ Preloader ] End -->

    <!-- [ Content ] Start -->
    <div class="authentication-wrapper authentication-2 ui-bg-cover ui-bg-overlay-container px-4" style="background-image: url('{{ asset('back-office/img/bg/21.jpg') }}');">
        <div class="ui-bg-overlay bg-dark opacity-25"></div>

        <div class="authentication-inner py-5">

            <div class="card">
                <div class="p-4 p-sm-5">
                    <!-- [ Logo ] Start -->
                    <div class="d-flex justify-content-center align-items-center pb-2 mb-4">
                        <div class="ui-w-60">
                            <div class="w-100 position-relative">
                                <img src="{{asset('back-office/img/logo-dark.png')}}" alt="Brand Logo" class="img-fluid">
                                <div class="clearfix"></div>
                            </div>
                        </div>
                    </div>
                    <!-- [ Logo ] End -->

                    <h5 class="text-center text-muted font-weight-normal mb-4">Create an Account</h5>

                    <!-- Form -->
                    {{ form_start(form,{attr:{novalidate:'novalidate'}}) }}
                        <div class="form-group">
                            <label class="form-label">First Name</label>
                            <div class="invalid-feedback">Please, enter your First Name!</div>
                                    {{ form_widget(form.FirstName,{'attr':{'class':'form-control'}}) }}
                            <div class="clearfix"></div>
                        </div>
                        <div class="form-group">
                            <label class="form-label">Last Name</label>
                            <div class="invalid-feedback">Please, enter your Last name!</div>
                            {{ form_widget(form.LastName,{'attr':{'class':'form-control'}}) }}
                            <div class="clearfix"></div>
                        </div>
                        <div class="form-group">
                            <label class="form-label">Type User</label>
                            {% if form.roles %}
                            {{ form_widget(form.roles) }}
                            {% else %}
                             {{ user.roles|json_encode }}
                             {% endif %}
                           <div class="clearfix"></div>
                        </div>
                        <div class="form-group">
                            <label class="form-label">Email</label>
                            {{ form_widget(form.email,{'attr':{'class':'form-control'}}) }}
                             <div class="invalid-feedback">Please enter a valid Email adddress!</div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="form-group">
                            <label class="form-label">Password</label>
                           {{ form_widget(form.password.first,{'attr':{'class':'form-control'}}) }}
                            <div class="invalid-feedback">Please enter your password!</div>
                        </div>
                         <div class="form-group">
                            <label class="form-label">Confirm Password</label>
                            {{ form_widget(form.password.second,{'attr':{'class':'form-control'}}) }}
                                    <div class="invalid-feedback">Please confirm your password!</div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="form-group">
                            <label class="form-label">Telephone</label>
                            {{ form_widget(form.telephone,{'attr':{'class':'form-control'}}) }}
                                    <div class="invalid-feedback">Please confirm your mobile number!</div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="form-group">
                            <label class="form-label">Date de Naissance</label>
                             {{ form_widget(form.birthdate,{'attr':{'class':'form-control'}}) }}
                                    <div class="invalid-feedback">Please you must be over 18 ans</div>
                            <div class="clearfix"></div>
                        </div>
                         <div class="form-group">
                            <label class="form-label">Gender</label>
                              {{ form_widget(form.gender,{'attr':{'class':'form-control'}}) }}
                                    <div class="invalid-feedback">Please confirm gender!</div>
      </fieldset>
                            <div class="clearfix"></div>
                        </div>
                        <button type="submit" class="btn btn-primary btn-block mt-4">Sign Up</button>
                        <div class="text-light small mt-4">
                            By clicking "Sign Up", you agree to our
                            <a href="javascript:void(0)">terms of service and privacy policy</a>. Weâ€™ll occasionally send you account related emails.
                        </div>
                     {{ form_end(form) }}
                    <!-- [ Form ] End -->
 
                </div>
                <div class="card-footer py-3 px-4 px-sm-5">
                    <div class="text-center text-muted">
                        Already have an account?
                        <a href="{{ path('app_login') }}">Sign In</a>
                    </div>
                </div>
               
            </div>

        </div>
    </div>
    <!-- / Content -->

  {% block js %}
    <!-- Core scripts -->
    <script src="{{asset('back-office/js/pace.js')}}"></script>
    <script src="{{asset('back-office/js/jquery-3.3.1.min.js')}}"></script>
    <script src="{{asset('back-office/libs/popper/popper.js')}}"></script>
    <script src="{{asset('back-office/js/bootstrap.js')}}"></script>
    <script src="{{asset('back-office/js/sidenav.js')}}"></script>
    <script src="{{asset('back-office/js/layout-helpers.js')}}"></script>
    <script src="{{asset('back-office/js/material-ripple.js')}}"></script>

    <!-- Libs -->
    <script src="{{asset('back-office/libs/perfect-scrollbar/perfect-scrollbar.js')}}"></script>

    <!-- Demo -->
    <script src="{{asset('back-office/js/demo.js')}}"></script>
    <script src="{{asset('back-office/js/analytics.js')}}"></script>
{% endblock %}
